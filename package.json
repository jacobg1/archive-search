{
  "name": "archive-search-lib",
  "version": "1.0.0",
  "description": "search helper for archive.orgs api",
  "main": "index.js",
  "scripts": {
    "test": "mocha ./tests/*.js || exit 0",
    "test:watch": "mocha --watch ./tests/*.js",
    "start": "webpack --mode=development",
    "build": "webpack --mode=production",
    "browser": "webpack --mode=development --config webpack.config.browser.js",
    "browser:dev": "webpack --mode=development --watch --config webpack.config.browser.js",
    "node": "node src/search.node.js",
    "node:dev": "webpack --mode=development --watch --config webpack.config.node.js",
    "dev": "concurrently --kill-others \"npm run browser:dev\" \"npm run node:dev\""
  },
  "browser": {
    "./dist/search.node.js": "./dist/search.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacobg1/archive-search-lib.git"
  },
  "keywords": [
    "archive.org"
  ],
  "author": "Jacob Greenwald",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jacobg1/archive-search-lib/issues"
  },
  "homepage": "https://github.com/jacobg1/archive-search-lib#readme",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "mocha": "^6.0.2",
    "nodemon": "^1.18.10",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "superagent": "^4.1.0",
    "superagent-jsonp": "^0.2.1"
  }
}
